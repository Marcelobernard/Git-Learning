import tkinter
from tkinter import messagebox as mb
from tkinter import ttk

root = tkinter.Tk()
root.title("Dropdown")
root.resizable(False, False)

n = tkinter.StringVar()
escolha = ttk.Combobox(root, textvariable = n)
escolha.pack()
escolha['values'] = ('CLT', 'MEI', 'SOCIO')
                         

root.iconify()
root.update()
root.deiconify()
root.mainloop()
